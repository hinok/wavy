{
  "name": "wavy-canvas",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "keywords": ["wavy", "waves", "canvas"],
  "author": "Dawid Karabin",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel serve watch",
    "serve": "live-server ./ --open=dist --port=8000 --ignore=node_modules",
    "watch": "npm-run-all --parallel watch:*",
    "watch:js": "watchify src/index.js -t babelify -o dist/bundle.js -v",
    "build": "npm-run-all --parallel build:*",
    "build:js": "browserify src/index.js -t babelify -o dist/bundle.js"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "eslint": "^1.10.1",
    "live-server": "^1.0.0",
    "npm-run-all": "^2.1.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "doc-ready": "^1.0.4",
    "document-offset": "^1.0.4",
    "easing-js": "^1.0.1",
    "hex-rgb": "^1.0.0",
    "hidpi-canvas": "github:jondavidjohn/hidpi-canvas-polyfill",
    "object-assign": "^4.1.0",
    "raf": "^3.2.0"
  }
}
